@model List<Customer>
@{
    ViewData["Title"] = "Login";
}

<h1>Logged In</h1>

<table border="1">
    <tr style="background-color:yellow">
        <th>Customer ID</th>
        <th>Customer Name</th>
        <th>Customer Email</th>
        <th>Customer Password</th>
        <th>Customer Phone Number</th>
    </tr>
    @foreach (var singlevoucher in Model)
    {
        <tr>
            <td>@singlevoucher.CustomerID</td>
            <td>@singlevoucher.CustomerName</td>
            <td>@singlevoucher.CustomerEmail</td>
            <td>@singlevoucher.CustomerPassword</td>
            <td>@singlevoucher.CustomerPhone</td>
        </tr>
    }
</table>